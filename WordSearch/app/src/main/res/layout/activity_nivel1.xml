<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/teal_700"
    android:orientation="vertical"
    tools:context=".Nivel1">

    <fragment
        android:id="@+id/fragHint"
        android:name="com.example.wordsearch.HintNivel1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_hint_nivel1" />

    <fragment
        android:id="@+id/fragWords"
        android:name="com.example.wordsearch.ListWords1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_list_words1" />

    <fragment
        android:id="@+id/fragTabel"
        android:name="com.example.wordsearch.TabelNivel1"
        android:layout_width="match_parent"
        android:layout_height="295dp"
        tools:layout="@layout/fragment_tabel_nivel1" />

    <fragment
        android:id="@+id/fragWord"
        android:name="com.example.wordsearch.Word1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_word1" />

</LinearLayout>